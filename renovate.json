{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended", ":disableDependencyDashboard"
  ],
  "constraints": {
    "java": "16"
  },
  "packageRules": [
    {
      "matchManagers": ["gradle-wrapper"],
      "allowedVersions": "<9.0.0"
    },
    {
      "matchDepNames": ["org.gradle.toolchains.foojay-resolver-convention"],
      "allowedVersions": "<1.0.0"
    },
    {
      "matchDepNames": ["dev.jorel:commandapi-bukkit-core"],
      "enabled": false
    },
    {
      "matchDepNames": ["com.vanniktech:gradle-maven-publish-plugin"],
      "allowedVersions": "<0.36.0"
    }
  ]
}
